{
    "builds": [
        {
            "src": "finance_tracker/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "finance_tracker/wsgi.py"
        }
    ]
}
